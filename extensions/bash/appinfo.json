{
  "type": "application",
  "name": "bash",
  "status_required": ["admin"],
  "ico": "app.svg",
  "html": "app.html",
  "css": ["app.css"],
  "script": ["app.js"],
  "executable": "app.py",
  "create_layout": [
    {
      "type": "row",
      "text": "Start path:",
      "elements": [
        {
          "type": "path",
          "arg": "path"
        }
      ]
    }, {
      "type": "row",
      "text": "CMD:",
      "elements": [
        {
          "type": "input",
          "arg": "cmd",
          "placeholder": "command",
          "value": "bash",
          "must_be_filled": false
        }
      ]
    }, {
      "type": "label",
      "text": "This bash terminal will be runned in subprocess."
    }
  ]
}
